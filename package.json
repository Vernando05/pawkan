{
  "name": "pawkan",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "serve-prd": "vue-cli-service serve --mode production",
    "test-ci:e2e": "vue-cli-service test:e2e --headless --url https://localhost:8081 --browser chrome",
    "test-ci": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test serve-prd https-get://localhost:8081 test-ci:e2e",
    "i18n-extract": "node ./node_modules/vue-i18n-extract/bin/vue-i18n-extract.js \"report\" \"-v\" \"./src/**/*.?(js|vue)\" \"-l\" \"./src/locales/*.json\""
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-link": "^1.2.14",
    "core-js": "^3.11.0",
    "eslint-config-typescript": "^3.0.0",
    "firebase": "^8.4.2",
    "firebaseui": "^4.8.0",
    "lodash": "^4.17.21",
    "register-service-worker": "^1.7.2",
    "vee-validate": "^3.4.5",
    "vue": "^2.6.12",
    "vue-apollo": "^3.0.7",
    "vue-cookies": "^1.7.4",
    "vue-gtag": "^1.14.0",
    "vue-i18n": "^8.24.3",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.5.1",
    "vue-tel-input": "^5.2.0",
    "vuetify": "^2.4.11",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@kazupon/vue-i18n-loader": "^0.5.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/vue": "^5.6.2",
    "@types/facebook-js-sdk": "^3.3.2",
    "@types/gapi.auth2": "0.0.54",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/vue-tel-input": "^2.1.2",
    "@types/webpack": "^4.41.27",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-e2e-cypress": "^4.5.12",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-plugin-pwa": "^4.5.12",
    "@vue/cli-plugin-router": "^4.5.12",
    "@vue/cli-plugin-typescript": "^4.5.12",
    "@vue/cli-plugin-unit-jest": "^4.5.12",
    "@vue/cli-plugin-vuex": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.1.4",
    "babel-plugin-macros": "^3.0.1",
    "dotenv": "^8.2.0",
    "es6-promise": "^4.2.8",
    "eslint": "^7.25.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-vue": "^7.9.0",
    "firebase-tools": "^9.10.0",
    "flush-promises": "^1.0.2",
    "graphql-tag": "^2.12.3",
    "isomorphic-fetch": "^3.0.0",
    "jest-transform-graphql": "^2.1.0",
    "node-sass": "^7.0.0",
    "require-context.macro": "^1.2.2",
    "sass": "^1.32.11",
    "sass-loader": "^10.1.1",
    "snapshot-diff": "^0.8.1",
    "start-server-and-test": "^1.12.1",
    "typescript": "^4.2.4",
    "vue-cli-plugin-apollo": "^0.22.2",
    "vue-cli-plugin-i18n": "^1.0.1",
    "vue-cli-plugin-vuetify": "^2.4.0",
    "vue-i18n-extract": "^1.2.1",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.12",
    "vue-typed-mixins": "^0.2.0",
    "vuetify-loader": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "plugins": [
      "jest"
    ],
    "rules": {
      "jest/no-disabled-tests": "warn",
      "jest/no-focused-tests": "error",
      "jest/no-identical-title": "error",
      "jest/prefer-to-have-length": "warn",
      "jest/valid-expect": "error",
      "jest/no-large-snapshots": [
        "warn",
        {
          "maxSize": 12,
          "inlineMaxSize": 6
        }
      ]
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel",
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/unit/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/tests/unit/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "./tests/unit/config.ts"
    ],
    "transformIgnorePatterns": [
      "./node_modules/(?!vee-validate/dist/rules)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/components/*.{js,vue}",
      "src/views/*.{js,vue}"
    ]
  }
}
